{{!-- {{#each this}} --}}
<div class="card">
  <div class="card-body">
      <h2 class="card-title">{{name}}</h2>
      <p>Capital: {{capital}}</p>
      <p>Population: {{population}}</p>
      <p>Languages: </p>
      <ul class="lang-list">
        {{#each languages}}
          <li class="lang-list-item">{{ this.name }}</li>
        {{/each}}
      </ul>
      
      <div class="card-img">
        <img src="{{flag}}" alt="{{name}}">
      </div>
  </div>
</div>
{{!-- {{/each}} --}}
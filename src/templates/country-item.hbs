{{!-- {{#each this}} --}}
<div class="card">  
  <div class="card-body">  
      <h2 class="card-title">{{name}}</h2>    
      <p><b>Capital:</b> {{capital}}</p>
      <p><b>Population:</b> {{population}}</p>
      <p><b>Languages:</b> </p>
      <ul class="lang-list">
        {{#each languages}}
          <li class="lang-list-item">{{ this.name }}</li>
        {{/each}}
      </ul>
  </div>
      
  <div class="card-img">
    <img src="{{flag}}" alt="{{name}}">
  </div>
</div>
{{!-- {{/each}} --}}